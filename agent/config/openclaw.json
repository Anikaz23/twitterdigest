{
  "model": "anthropic/claude-sonnet-4-20250514",
  "anthropicApiKey": "${ANTHROPIC_API_KEY}",
  "gateway": {
    "port": 18789,
    "host": "127.0.0.1"
  },
  "workspace": {
    "root": "/root/.openclaw/workspace"
  },
  "browser": {
    "executablePath": "/usr/bin/chromium",
    "headless": true,
    "args": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-gpu"
    ]
  },
  "cron": {
    "enabled": true
  }
}
